{
  "name": "eventfan",
  "version": "1.0.5",
  "description": "Send analytics events to multiple destinations (Google Analytics, Facebook...).",
  "repository": "https://github.com/alan-cooney/eventfan",
  "author": "Alan Cooney <alan@skyhookadventure.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn buildBrowser && yarn buildNode && yarn buildTypes",
    "buildBrowser": "parcel build",
    "buildNode": "babel src -d dist/module --extensions '.tsx,.ts' --config-file ./babelNode.json",
    "buildTypes": "tsc --emitDeclarationOnly",
    "bump": "./node_modules/.bin/versiony package.json --patch",
    "coverage": "./node_modules/.bin/jest --collect-coverage",
    "dev": "yarn react",
    "lint": "./node_modules/.bin/eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "playwright": "playwright test --config=src/e2e/playwright.config.ts",
    "react": "yarn vite --config ./src/e2e/react/vite.config.js",
    "test": "./node_modules/.bin/jest",
    "browser": "yarn vite --config ./src/e2e/browser/vite.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@playwright/test": "^1.16.3",
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.111",
    "@swc/jest": "^0.2.5",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-react": "^1.1.0",
    "babel-plugin-add-import-extension": "^1.6.0",
    "change-case": "^4.1.2",
    "cross-fetch": "^3.1.4",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "hygen": "^6.1.0",
    "jest": "^27.3.1",
    "live-server": "^1.2.1",
    "parcel": "^2.0.1",
    "parcel-plugin-externals": "^0.5.2",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "playwright": "^1.16.3",
    "postal-address-field-names": "^1.0.3",
    "prettier": "^2.4.1",
    "quicktype-core": "^6.0.70",
    "react-bootstrap": "^2.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^4.0.3",
    "recursive-readdir-sync": "^1.0.6",
    "tiny-glob": "^0.2.9",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.2",
    "versiony-cli": "^1.3.0",
    "vite": "^2.6.14"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/*unit.test.ts",
      "**/*unit.test.tsx",
      "**/*int.test.ts",
      "**/*int.test.tsx"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "sourceMaps": true
        }
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "posthog-js": "^1.16.6",
    "react": "^17.0.2",
    "uuid": "^8.3.2"
  },
  "main": "./dist/main/index.js",
  "module": "./dist/module/index.js",
  "types": "./dist/module/index.d.ts",
  "targets": {
    "browser": {
      "context": "browser",
      "outputFormat": "esmodule",
      "isLibrary": true,
      "source": "./src/browser.ts",
      "optimize": true,
      "includeNodeModules": {
        "react": false
      },
      "sourceMap": true,
      "distDir": "./dist/browser"
    },
    "legacyBrowser": {
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      },
      "context": "browser",
      "outputFormat": "global",
      "isLibrary": true,
      "source": "./src/browser.ts",
      "optimize": true,
      "includeNodeModules": {
        "react": false
      },
      "sourceMap": true,
      "distDir": "./dist/legacy"
    },
    "main": {
      "engines": {
        "node": ">= 12"
      },
      "context": "node",
      "isLibrary": true,
      "source": "./src/index.ts",
      "optimize": false,
      "includeNodeModules": false,
      "sourceMap": false,
      "distDir": "./dist/main"
    }
  }
}